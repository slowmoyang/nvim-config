{
  "load json": {
    "prefix": ["loadjson"],
    "body": [
      "with open(${1:path}) as stream:",
      "\t${2:data} = json.load(stream)"
    ]
  },
  "dump json": {
    "prefix": ["dumpjson"],
    "body": [
      "with open(${1:path}) as stream:",
      "\tjson.dump(${2:data}, stream, indent=2)"
    ]
  }
}
