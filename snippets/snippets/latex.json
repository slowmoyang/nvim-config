{
  "minimpage": {
    "prefix": "minipage",
    "body": [
      "\\begin{minipage}{${2:0.48\\textwidth}}",
      "\t${1}",
      "\\end{minipage}"
    ]
  },
  "import": {
    "prefix": "import",
    "body": "\\import{${1:full_path}}{${2:file}}",
    "description": "TODO"
  },
  "subimport": {
    "prefix": "subimport",
    "body": "\\subimport{${1:path_extension}}{${2:file}}",
    "description": "TODO"
  },
  "graphicspath": {
    "prefix": "graphicspath",
    "body": [
      "\\makeatletter",
      "\t\\graphicspath{ {\\import@path} }",
      "\\makeatother"
    ],
    "description": "http://latexref.xyz/_005cgraphicspath.html"
  }
}
