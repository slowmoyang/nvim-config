{
  "fragile": {
    "prefix": "fragile_frame",
    "body": [
      "\\begin{frame}[fragile]{${1:title}}",
      "\t${2:TODO}",
      "\\end{frame}"
    ]
  },
  "standout": {
    "prefix": "standout",
    "body": [
      "\\begin{frame}[standout]",
      "\t${2:Thank You}",
      "\\end{frame}"
    ],
    "description": "standout"
  },
  "columns": {
    "prefix": "columns",
    "body": [
      "\\begin{columns}",
      "\t\\column{0.5\\textwidth}",
      "\t\tcolumn1",
      "\t%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\t\\column{0.5\\textwidth}",
      "\t\tcolumn2",
      "\\end{columns}"
    ]
  }
}
